package f3.media.scene;

public class WebBrowser is WebBrowserNode 
{
    var urlInvalid = true;
    public var url is String on invalidate {
	urlInvalid = true;
    }

    override function update {
	super.update();
	if (urlInvalid) {
	    urlInvalid = false;
	    tex.setURL(url);
	    super.update();
	}
    }
}